{
  "name": "s-bars",
  "description": "Static website for `Snelnij Bars` company",
  "private": true,
  "version": "1.0.0",
  "license": "MIT",
  "author": "Oleksii Tymoshenko <buildmnt@gmail.com>",
  "keywords": [
    "gatsby",
    "gatsbyjs",
    "blog",
    "landing",
    "i18n",
    "emotion"
  ],
  "main": "n/a",
  "homepage": "https://github.com/alextim/s-bars",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alextim/s-bars.git"
  },
  "bugs": {
    "url": "https://github.com/alextim/s-bars/issues"
  },
  "scripts": {
    "build": "gatsby build",
    "build:incremental": "cross-env GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "build:ua": "cross-env LOCALES=ua ONLY=/ gatsby build",
    "dev": "gatsby develop -o",
    "clean": "gatsby clean",
    "serve": "gatsby serve -o",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write \"src/**/*.{js, jsx}\"",
    "git:tracked": "git ls-tree -r master --name-only",
    "git:log": "git log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short",
    "check:licenses": "license-checker --production --summary --failOn=GPLv3",
    "check:security": "audit-ci --critical",
    "test": "cross-env NODE_ENV=test echo \"Write tests!\"",
    "testbuild": "npm run test && npm run build",
    "ncu": "ncu -u --timeout 150000",
    "trans": "node scripts/translations-json-to-yaml",
    "upd": "scripts/content-update.bat",
    "postinstall": "husky install",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "@alextim/at-seo": "^0.4.32",
    "@alextim/csp": "^0.1.18",
    "@alextim/gatsby-plugin-blogcore": "^0.8.37",
    "@alextim/gatsby-plugin-sitemap": "^0.4.21",
    "@alextim/i18n-utils": "^0.1.16",
    "@alextim/slugify": "^0.1.16",
    "@alextim/translit": "^0.1.17",
    "@alextim/utils": "^0.1.26",
    "@emotion/react": "^11.13.5",
    "@sendgrid/mail": "^8.1.4",
    "disable-scroll": "^0.6.0",
    "dotenv": "^16.4.5",
    "email-validator": "^2.0.4",
    "gatsby": "^5.14.0",
    "gatsby-plugin-emotion": "^8.14.0",
    "gatsby-plugin-feed": "^5.14.0",
    "gatsby-plugin-google-analytics": "^5.14.0",
    "gatsby-plugin-image": "^3.14.0",
    "gatsby-plugin-manifest": "^5.14.0",
    "gatsby-plugin-netlify": "^5.1.1",
    "gatsby-plugin-sharp": "^5.14.0",
    "gatsby-remark-images": "^7.14.0",
    "gatsby-source-filesystem": "^5.14.0",
    "gatsby-transformer-remark": "^6.14.0",
    "gatsby-transformer-sharp": "^5.14.0",
    "gatsby-transformer-yaml": "^5.14.0",
    "node-fetch": "^3.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.2.0",
    "react-multi-carousel": "^2.8.5"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/eslint-parser": "^7.25.9",
    "@babel/preset-react": "^7.25.9",
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@emotion/eslint-plugin": "^11.12.0",
    "babel-preset-gatsby": "^3.13.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.16.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-custom-alias": "^1.3.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gatsby-plugin-react-svg": "^3.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "prettier": "^3.4.1",
    "preval.macro": "^5.0.0",
    "typescript": "^5.7.2"
  },
  "comments": {
    "content/data/locales/translations/src/*.json": [
      "npm run translations:json2yaml",
      "git add ."
    ],
    "browserslist": [
      ">0.25%",
      "not dead"
    ],
    "audit-ci": "",
    "gatsby-plugin-webpack-bundle-analyser-v2": "",
    "license-checker": "",
    "react-image-lightbox": "",
    "git": "git fsck, git gc, git prune"
  }
}
