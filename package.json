{
  "name": "s-bars",
  "description": "Static website for `Snelnij Bars` company",
  "private": true,
  "version": "0.3.3",
  "license": "MIT",
  "author": "Oleksii Tymoshenko <buildmnt@gmail.com>",
  "keywords": [
    "gatsby",
    "gatsbyjs",
    "blog",
    "landing",
    "i18n",
    "eslint",
    "emotion"
  ],
  "main": "n/a",
  "homepage": "https://github.com/alextim/s-bars",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alextim/s-bars.git"
  },
  "bugs": {
    "url": "https://github.com/alextim/s-bars/issues"
  },
  "scripts": {
    "build": "gatsby build",
    "build:incremental": "cross-env GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "build:ua": "cross-env LOCALES=ua ONLY=/ gatsby build",
    "dev": "gatsby develop -o",
    "clean": "gatsby clean",
    "serve": "gatsby serve -o",
    "lint": "eslint src/",
    "lint:write": "eslint src/ --fix",
    "prettier": "prettier --write \"src/**/*.{js, jsx}\"",
    "git:tracked": "git ls-tree -r master --name-only",
    "git:log": "git log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short",
    "check:licenses": "license-checker --production --summary --failOn=GPLv3",
    "check:security": "audit-ci --critical",
    "test": "cross-env NODE_ENV=test yarn run eslint",
    "testbuild": "yarn test && yarn build",
    "ncu": "ncu -u --timeout 150000",
    "trans": "node scripts/translations-json-to-yaml",
    "upd": "scripts/content-update.bat",
    "postinstall": "husky install",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "@alextim/at-seo": "^0.4.28",
    "@alextim/csp": "^0.1.16",
    "@alextim/gatsby-plugin-blogcore": "^0.8.33",
    "@alextim/gatsby-plugin-sitemap": "^0.4.19",
    "@alextim/i18n-utils": "^0.1.14",
    "@alextim/slugify": "^0.1.14",
    "@alextim/translit": "^0.1.15",
    "@alextim/utils": "^0.1.23",
    "@emotion/react": "^11.9.3",
    "@sendgrid/mail": "^7.7.0",
    "disable-scroll": "^0.6.0",
    "dotenv": "^16.0.1",
    "email-validator": "^2.0.4",
    "gatsby": "^4.17.2",
    "gatsby-plugin-emotion": "^7.17.0",
    "gatsby-plugin-feed": "^4.15.1",
    "gatsby-plugin-google-analytics": "^4.17.0",
    "gatsby-plugin-image": "^2.17.0",
    "gatsby-plugin-manifest": "^4.17.0",
    "gatsby-plugin-netlify": "^5.0.1",
    "gatsby-plugin-preact": "^6.17.0",
    "gatsby-plugin-react-helmet": "^5.17.0",
    "gatsby-plugin-remove-generator": "^1.2.0",
    "gatsby-plugin-sharp": "^4.17.0",
    "gatsby-remark-images": "^6.17.0",
    "gatsby-source-filesystem": "^4.17.0",
    "gatsby-transformer-remark": "^5.17.0",
    "gatsby-transformer-sharp": "^4.17.0",
    "gatsby-transformer-yaml": "^4.17.0",
    "node-fetch": "^3.2.6",
    "preact": "^10.10.0",
    "preact-render-to-string": "^5.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-multi-carousel": "^2.8.2"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/eslint-parser": "^7.18.2",
    "@babel/preset-react": "^7.18.6",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@emotion/eslint-plugin": "^11.7.0",
    "@hot-loader/react-dom": "^17.0.2",
    "@testing-library/react-hooks": "^8.0.1",
    "babel-preset-gatsby": "^2.15.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.18.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-custom-alias": "^1.3.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gatsby-plugin-react-svg": "^3.1.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "preval.macro": "^5.0.0"
  },
  "comments": {
    "content/data/locales/translations/src/*.json": [
      "yarn translations:json2yaml",
      "git add ."
    ],
    "browserslist": [
      ">0.25%",
      "not dead"
    ],
    "audit-ci": "",
    "gatsby-plugin-webpack-bundle-analyser-v2": "",
    "license-checker": "",
    "react-image-lightbox": "",
    "git": "git fsck, git gc, git prune"
  }
}
