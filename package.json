{
  "name": "s-bars",
  "description": "Static website for `Snelnij Bars` company",
  "private": true,
  "version": "0.0.2",
  "license": "MIT",
  "author": "Oleksii Tymoshenko <buildmnt@gmail.com>",
  "keywords": [
    "gatsby",
    "gatsbyjs",
    "blog",
    "landing",
    "i18n",
    "eslint",
    "emotion"
  ],
  "main": "n/a",
  "homepage": "https://github.com/alextim/s-bars",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alextim/s-bars.git"
  },
  "bugs": {
    "url": "https://github.com/alextim/s-bars/issues"
  },
  "scripts": {
    "build": "gatsby build",
    "build:incremental": "cross-env GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "build:ua": "cross-env LOCALES=ua ONLY=/ gatsby build",
    "dev": "gatsby develop -o",
    "clean": "gatsby clean",
    "serve": "gatsby serve -o",
    "lint": "eslint src/",
    "lint:write": "eslint --debug src/ --fix",
    "stylelint": "stylelint --syntax css-in-js src/**/*.{js,jsx,ts,tsx,css,scss}",
    "format": "prettier-eslint --write 'src/**/*.{js,jsx,json,md}'",
    "git:tracked": "git ls-tree -r master --name-only",
    "git:log": "git log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short",
    "check:licenses": "license-checker --production --summary --failOn=GPLv3",
    "check:security": "audit-ci --critical",
    "test": "cross-env NODE_ENV=test yarn run eslint",
    "testbuild": "yarn test && yarn build",
    "ncu": "ncu -u --timeout 150000",
    "trans": "node scripts/translations-json-to-yaml",
    "upd": "scripts/content-update.bat",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@alextim/at-blog": "^0.2.7",
    "@alextim/at-seo": "^0.1.14",
    "@alextim/at-site-core": "^0.1.4",
    "@alextim/at-sitemap": "^0.2.8",
    "@alextim/csp": "^0.0.1",
    "@alextim/slugify": "^0.1.0",
    "@alextim/translit": "^0.1.1",
    "@alextim/utils": "^0.1.0",
    "@emotion/react": "^11.1.5",
    "@sendgrid/mail": "^7.4.2",
    "disable-scroll": "^0.6.0",
    "dotenv": "^8.2.0",
    "email-validator": "^2.0.4",
    "emotion-theming": "^11.0.0",
    "gatsby": "^3.3.0",
    "gatsby-plugin-emotion": "^6.3.0",
    "gatsby-plugin-feed": "^3.3.0",
    "gatsby-plugin-google-analytics": "^3.3.0",
    "gatsby-plugin-image": "^1.3.0",
    "gatsby-plugin-manifest": "^3.3.0",
    "gatsby-plugin-netlify": "^3.3.0",
    "gatsby-plugin-portal": "^1.0.7",
    "gatsby-plugin-react-helmet": "^4.3.0",
    "gatsby-plugin-remove-generator": "^1.1.0",
    "gatsby-plugin-sharp": "^3.3.0",
    "gatsby-remark-images": "^5.0.0",
    "gatsby-remark-relative-images": "^2.0.2",
    "gatsby-source-filesystem": "^3.3.0",
    "gatsby-transformer-remark": "^4.0.0",
    "gatsby-transformer-sharp": "^3.3.0",
    "gatsby-transformer-yaml": "^3.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "swiper": "^6.5.7"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/eslint-parser": "^7.13.14",
    "@emotion/eslint-plugin": "^11.2.0",
    "@hot-loader/react-dom": "^17.0.1",
    "@testing-library/react-hooks": "^5.1.1",
    "babel-preset-gatsby": "^1.3.0",
    "cross-env": "^7.0.3",
    "eslint": "7.24.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "7.23.2",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-webpack-plugin": "^2.5.4",
    "gatsby-plugin-eslint": "^3.0.0",
    "gatsby-plugin-react-svg": "^3.0.1",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "prettier-eslint": "^12.0.0",
    "prettier-eslint-cli": "^5.0.1",
    "preval.macro": "^5.0.0",
    "stylelint": "^13.12.0",
    "stylelint-config-standard": "^21.0.0"
  },
  "comments": {
    "content/data/locales/translations/src/*.json": [
      "yarn translations:json2yaml",
      "git add ."
    ],
    "browserslist": [
      ">0.25%",
      "not dead"
    ],
    "audit-ci": "",
    "gatsby-plugin-webpack-bundle-analyser-v2": "",
    "license-checker": "",
    "react-image-lightbox": ""
  }
}
