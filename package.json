{
  "name": "s-bars",
  "description": "Snelnij Bars",
  "private": true,
  "version": "0.0.1",
  "license": "MIT",
  "author": "Oleksii Tymoshenko <buildmnt@gmail.com>",
  "keywords": [
    "gatsby",
    "gatsbyjs",
    "blog",
    "landing",
    "i18n",
    "eslint",
    "emotion"
  ],
  "main": "n/a",
  "homepage": "https://github.com/alextim/s-bars",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alextim/s-bars.git"
  },
  "bugs": {
    "url": "https://github.com/alextim/s-bars/issues"
  },
  "scripts": {
    "build": "gatsby build",
    "build:incremental": "cross-env GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "build:en": "cross-env LOCALES=en ONLY=/ gatsby build",
    "dev": "gatsby develop -o",
    "clean": "gatsby clean",
    "serve": "gatsby serve -o",
    "stylelint": "stylelint --syntax css-in-js src/**/*.{js,jsx,ts,tsx,css,scss}",
    "format": "prettier-eslint --write 'src/**/*.{js,jsx,json,md}'",
    "git:tracked": "git ls-tree -r master --name-only",
    "check:licenses": "license-checker --production --summary --failOn=GPLv3",
    "check:security": "audit-ci --critical",
    "test": "cross-env NODE_ENV=test yarn run eslint",
    "testbuild": "yarn test && yarn build",
    "ncu": "ncu -u --timeout 120000",
    "translations:json2yaml": "node scripts/translations-json-to-yaml"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src/**/*.{js,jsx}": [
      "eslint --fix",
      "prettier-eslint --write"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.1.4",
    "@loadable/component": "^5.14.1",
    "@sendgrid/mail": "^7.4.2",
    "disable-scroll": "^0.6.0",
    "dotenv": "^8.2.0",
    "email-validator": "^2.0.4",
    "emotion-theming": "^11.0.0",
    "gatsby": "^2.31.1",
    "gatsby-image": "^2.10.0",
    "gatsby-plugin-emotion": "^5.3.0",
    "gatsby-plugin-google-analytics": "^2.10.0",
    "gatsby-plugin-manifest": "^2.11.0",
    "gatsby-plugin-portal": "^1.0.7",
    "gatsby-plugin-react-helmet": "^3.9.0",
    "gatsby-plugin-sharp": "^2.13.4",
    "gatsby-plugin-sitemap": "^2.11.0",
    "gatsby-remark-images": "^3.10.0",
    "gatsby-remark-relative-images": "^2.0.2",
    "gatsby-source-filesystem": "^2.10.0",
    "gatsby-transformer-remark": "^2.15.0",
    "gatsby-transformer-sharp": "^2.11.0",
    "gatsby-transformer-yaml": "^2.10.0",
    "lazysizes": "^5.3.0",
    "lodash": "^4.17.20",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-image-lightbox": "^5.1.1",
    "react-slick": "^0.28.0",
    "slick-carousel": "^1.8.1"
  },
  "devDependencies": {
    "@emotion/eslint-plugin": "^11.0.0",
    "@hot-loader/react-dom": "^17.0.1",
    "@testing-library/react-hooks": "^5.0.3",
    "audit-ci": "^3.1.1",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "eslint": "7.19.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-webpack-plugin": "^2.4.3",
    "gatsby-plugin-eslint": "^3.0.0",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.20",
    "husky": "^4.3.8",
    "license-checker": "^25.0.1",
    "lint-staged": "^10.5.3",
    "netlify-cli": "^3.4.6",
    "prettier": "^2.2.1",
    "prettier-eslint": "^12.0.0",
    "preval.macro": "^5.0.0",
    "stylelint": "^13.9.0",
    "stylelint-config-standard": "^20.0.0"
  }
}
